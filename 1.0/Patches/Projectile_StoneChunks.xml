<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Make Rock to Projectile -->
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- StoneChunks -->
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[@Name="ChunkRockBase"]</xpath>
				<value>
					<projectileWhenLoaded>VFES_Artillery_Rock</projectileWhenLoaded>
				</value>
			</li>
		</operations>	
	</Operation>

</Patch>
